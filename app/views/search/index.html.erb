<h1>Stations</h1>

<div class="station">
  <% @stations.each do |station| %>
    <li><%= station.name %>
        <li> Address:<%= station.street_address %>
                     <%= station.state %>
                     <%= station.zip %></li>
        <li>Fuel Type:<%= station.fuel_type %></li>
        <li>Distance:<%= station.distance %></li>
        <li>Access Times:<%= station.access %></li>
        <% end %>
    </li>
